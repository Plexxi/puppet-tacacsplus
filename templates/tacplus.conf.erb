<% if @server -%>
<% Array(@server).each do |serv| -%>
server=<%= serv['addr'] %>
secret=<%= serv['secret'] %>
<% end -%>
<%end -%>
login=<%= @login %>
service=<%= @service %>
protocol=<%= @protocol %>
timeout=<%= @timeout %>
